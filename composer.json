{
    "name": "transip/transip-api-symfony",
    "description": "TransIP API client integration for Symfony",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Transip\\Bundle\\RestApi\\": "src"
        }
    },
    "authors": [
        {
            "name": "Sem Schilder",
            "email": "sem.schilder@team.blue"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "transip/transip-api-php": "^6.9.2",
        "psr/http-client":  "^1.0",
        "nyholm/psr7": "^1.4",
        "symfony/framework-bundle": "^4.4 ||^5.3",
        "php-http/httplug": "^1.1 || ^2.0",
        "php-http/discovery": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^1.6 || ^2.0"
    },
    "require-dev": {
        "symfony/http-client": "^4.4 || ^5.3",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^0.12.99"
    }
}
